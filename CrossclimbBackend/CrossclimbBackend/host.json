{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensions": {
        "http": {
            "cors": {
                "allowedOrigins": [
                    "https://crossclimb-game.com",
                    "http://localhost:3000",
                    "http://127.0.0.1:3000"
                ],
                "allowedMethods": [
                    "GET",
                    "POST",
                    "PUT",
                    "DELETE",
                    "OPTIONS"
                ],
                "allowedHeaders": [
                    "Content-Type",
                    "Authorization",
                    "X-Requested-With"
                ],
                "allowCredentials": false
            }
        }
    }
}